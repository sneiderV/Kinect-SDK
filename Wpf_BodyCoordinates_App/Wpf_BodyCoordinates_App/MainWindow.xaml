<Window x:Class="Wpf_BodyCoordinates_App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_BodyCoordinates_App"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Mid Spine X" HorizontalAlignment="Left" Margin="122,118,0,0" VerticalAlignment="Top"/>
        <Label Content="Mid Spine Y" HorizontalAlignment="Left" Margin="122,149,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.028,0.569"/>
        <Label Content="Mid Spine Z" HorizontalAlignment="Left" Margin="122,180,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="tb_spineX" HorizontalAlignment="Left" Margin="216,123,0,0" TextWrapping="Wrap" Text="tb_spineX" VerticalAlignment="Top"/>
        <TextBlock x:Name="tb_spineY" HorizontalAlignment="Left" Margin="216,154,0,0" TextWrapping="Wrap" Text="tb_spineY" VerticalAlignment="Top"/>
        <TextBlock x:Name="tb_spineZ" HorizontalAlignment="Left" Margin="216,185,0,0" TextWrapping="Wrap" Text="tb_spineY" VerticalAlignment="Top"/>

    </Grid>
</Window>
